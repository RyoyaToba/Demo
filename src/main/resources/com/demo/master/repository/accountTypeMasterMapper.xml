<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >

<mapper namespace="com.demo.master.repository.AccountTypeMasterMapper" >

    <select id="selectAll" resultType="com.demo.master.entity.AccountTypeMaster">
        SELECT
            id
            ,name
            ,created_by
            ,created_date
            ,updated_by
            ,updated_date
        FROM
            account_type_master
    </select>

</mapper>